        <Param val="import math&amp;#10;&amp;#10;optimal_moves_wythoffs = [(0,0),(1,2),(2,1),(3,5),(5,3),(4,7), (7,4),(6,10),(10,6),(8,13),(13,8)]&amp;#10;&amp;#10;optimal_moves_nim = []&amp;#10;for i in range(15):&amp;#10;    optimal_moves_nim.append((i,i))&amp;#10;&amp;#10;optimal_moves_euclid = []&amp;#10;for r in range(15):&amp;#10;    for c in range(15):&amp;#10;        if  r == c == 0:&amp;#10;            optimal_moves_euclid.append((r,c))&amp;#10;        elif min(r,c) / max(r,c) &gt; (1 + math.sqrt(5)) / 2:&amp;#10;            optimal_moves_euclid.append((r,c))&amp;#10;&amp;#10;def euclid_move(pos1, pos2):&amp;#10;    (r1, c1) = pos1&amp;#10;    (r2, c2) = pos2&amp;#10;    &amp;#10;    if r2 &gt; r1 or c2 &gt; c1: return False&amp;#10;    &amp;#10;    elif r1 == 0 or c1 == 0:&amp;#10;        &amp;#10;        if r1 == 0:&amp;#10;            if c2 &lt; c1: return True&amp;#10;            else: return False&amp;#10;        if c1 == 0:&amp;#10;            if r2 &lt; r1: return True&amp;#10;            else: return False&amp;#10;    &amp;#10;    else:&amp;#10;        if r1 == c1:&amp;#10;            if r2 == 0 and c2 == c1: return True&amp;#10;            if c2 == 0 and r2 == r1: return True&amp;#10;            return False&amp;#10;        &amp;#10;        if r1 &gt; c1 and r2 &lt; r1:&amp;#10;            if (r1 - r2) % c1 == 0: return True&amp;#10;            else: return False&amp;#10;        &amp;#10;        if c1 &gt; r1 and c2 &lt; c1:&amp;#10;            if (c1 - c2) % r1 == 0: return True&amp;#10;            else: return False&amp;#10;    &amp;#10;    return False&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="import * as math from 'math';&amp;#10;optimal_moves_wythoffs = [[0, 0], [1, 2], [2, 1], [3, 5], [5, 3], [4, 7], [7, 4], [6, 10], [10, 6], [8, 13], [13, 8]];&amp;#10;optimal_moves_nim = [];&amp;#10;for (var i, _pj_c = 0, _pj_a = util.range(15), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    i = _pj_a[_pj_c];&amp;#10;    optimal_moves_nim.push([i, i]);&amp;#10;}&amp;#10;optimal_moves_euclid = [];&amp;#10;for (var r, _pj_c = 0, _pj_a = util.range(15), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    r = _pj_a[_pj_c];&amp;#10;    for (var c, _pj_f = 0, _pj_d = util.range(15), _pj_e = _pj_d.length; (_pj_f &lt; _pj_e); _pj_f += 1) {&amp;#10;        c = _pj_d[_pj_f];&amp;#10;        if (((r === c) &amp;&amp; (c === 0))) {&amp;#10;            optimal_moves_euclid.push([r, c]);&amp;#10;        } else {&amp;#10;            if (((Math.min(r, c) / Math.max(r, c)) &gt; ((1 + Math.sqrt(5)) / 2))) {&amp;#10;                optimal_moves_euclid.push([r, c]);&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;function euclid_move(pos1, pos2) {&amp;#10;    var c1, c2, r1, r2;&amp;#10;    [r1, c1] = pos1;&amp;#10;    [r2, c2] = pos2;&amp;#10;    if (((r2 &gt; r1) || (c2 &gt; c1))) {&amp;#10;        return false;&amp;#10;    } else {&amp;#10;        if (((r1 === 0) || (c1 === 0))) {&amp;#10;            if ((r1 === 0)) {&amp;#10;                if ((c2 &lt; c1)) {&amp;#10;                    return true;&amp;#10;                } else {&amp;#10;                    return false;&amp;#10;                }&amp;#10;            }&amp;#10;            if ((c1 === 0)) {&amp;#10;                if ((r2 &lt; r1)) {&amp;#10;                    return true;&amp;#10;                } else {&amp;#10;                    return false;&amp;#10;                }&amp;#10;            }&amp;#10;        } else {&amp;#10;            if ((r1 === c1)) {&amp;#10;                if (((r2 === 0) &amp;&amp; (c2 === c1))) {&amp;#10;                    return true;&amp;#10;                }&amp;#10;                if (((c2 === 0) &amp;&amp; (r2 === r1))) {&amp;#10;                    return true;&amp;#10;                }&amp;#10;                return false;&amp;#10;            }&amp;#10;            if (((r1 &gt; c1) &amp;&amp; (r2 &lt; r1))) {&amp;#10;                if ((((r1 - r2) % c1) === 0)) {&amp;#10;                    return true;&amp;#10;                } else {&amp;#10;                    return false;&amp;#10;                }&amp;#10;            }&amp;#10;            if (((c1 &gt; r1) &amp;&amp; (c2 &lt; c1))) {&amp;#10;                if ((((c1 - c2) % r1) === 0)) {&amp;#10;                    return true;&amp;#10;                } else {&amp;#10;                    return false;&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;    return false;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="var _pj;&amp;#10;function _pj_snippets(container) {&amp;#10;    function in_es6(left, right) {&amp;#10;        if (((right instanceof Array) || ((typeof right) === &quot;string&quot;))) {&amp;#10;            return (right.indexOf(left) &gt; (- 1));&amp;#10;        } else {&amp;#10;            if (((right instanceof Map) || (right instanceof Set) || (right instanceof WeakMap) || (right instanceof WeakSet))) {&amp;#10;                return right.has(left);&amp;#10;            } else {&amp;#10;                return (left in right);&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;    container[&quot;in_es6&quot;] = in_es6;&amp;#10;    return container;&amp;#10;}&amp;#10;_pj = {};&amp;#10;_pj_snippets(_pj);&amp;#10;new psychoJS.eventManager.Mouse({&quot;visible&quot;: false});&amp;#10;if (practice_turns.finished) {&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;row = row_new;&amp;#10;col = col_new;&amp;#10;moves = [];&amp;#10;Q_table = [];&amp;#10;if ((section !== 4)) {&amp;#10;    optimal_moves = optimal_moves_wythoffs;&amp;#10;} else {&amp;#10;    if (((Number.parseInt(expInfo[&quot;session&quot;]) % 2) === 0)) {&amp;#10;        optimal_moves = optimal_moves_euclid;&amp;#10;    } else {&amp;#10;        optimal_moves = optimal_moves_nim;&amp;#10;    }&amp;#10;}&amp;#10;for (var r, _pj_c = 0, _pj_a = util.range(row), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    r = _pj_a[_pj_c];&amp;#10;    move = [r, col];&amp;#10;    if ((((section !== 4) || ((Number.parseInt(expInfo[&quot;session&quot;]) % 2) === 1)) || euclid_move([row, col], move))) {&amp;#10;        moves.push(move);&amp;#10;        if (_pj.in_es6(move, optimal_moves)) {&amp;#10;            Q_table.push(1);&amp;#10;        } else {&amp;#10;            Q_table.push(0);&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;for (var c, _pj_c = 0, _pj_a = util.range(col), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    c = _pj_a[_pj_c];&